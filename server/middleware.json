{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "helmet": {
      "params": {
        "xssFilter": true,
        "frameguard": { "action": "deny" },
        "hsts": {
          "maxAge": 0,
          "includeSubDomains": true
        },
        "hidePoweredBy": true,
        "ieNoOpen": true,
        "noSniff": true,
        "noCache": false
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {},
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {}
  }
}
